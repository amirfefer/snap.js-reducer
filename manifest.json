{
  "manifest_version": 2,

  "name": "Snapshot Reducer Plugin",
  "description": "This extension reduces snapshots files from the total addition diff in github",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "unreduce.png",
    "reduced.png"
],


  "content_scripts": [
    {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["getAmount.js"],
        "css": [ "styles.css" ]
    }
   ],
  "permissions": [
   "activeTab"
   ]
}